{
  "name": "progress",  
  "version": "1.0.1",  
  "manifest_version": 2,  
  "description": "like youtube progress for chrome",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery.min.js","nprogress.min.js","myscript.js"],
      "css": ["nprogress.min.css"],
      "run_at": "document_start",
      "all_frames": true
      
    }
  ]

}
